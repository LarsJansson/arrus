logging_level: TRACE

# It will be searched for in the current directory, then in the directory
# pointed to by ARRUS_PATH env variable.
dictionary_file: "default.dict"

us4rlite: {
    probe_model_id: {
        manufacturer: "esaote"
        name: "sl1543"
    }

    adapter_model_id: {
        manufacturer: "us4us"
        name: "esaote2"
    }

    # Default initial values.
    gain_settings: {
        lna_gain: 12
        pga_gain: 24
        tgc_curve_linear: {
            intercept: 14
            slope: 2e2
        }
    }

    lpf_cutoff: 10e6
    active_termination: 200
}


